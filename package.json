{
  "name": "nuxt-oidc-auth",
  "private": false,
  "version": "0.12.8",
  "description": "OIDC authentication module for Nuxt SSR",
  "repository": "itpropro/nuxt-oidc-auth",
  "homepage": "https://github.com/itpropro/nuxt-oidc-auth#readme",
  "license": "MIT",
  "packageManager": "pnpm@9.0.6",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./package.json": "./package.json",
    "./runtime/*": "./dist/runtime/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "client:build": "nuxi generate client",
    "client:dev": "nuxi dev client --port 3300",
    "dev": "nuxi dev playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground && nuxi prepare client",
    "release": "pnpm lint && pnpm prepack",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/devtools-kit": "^1.6.2",
    "consola": "^3.2.3",
    "defu": "^6.1.4",
    "h3": "^1.13.0",
    "jose": "^5.9.6",
    "ofetch": "^1.4.1",
    "scule": "^1.3.0",
    "sirv": "^2.0.4",
    "ufo": "^1.5.4",
    "uncrypto": "^0.1.3",
    "unstorage": "^1.12.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.6.2",
    "@nuxt/devtools-ui-kit": "^1.6.2",
    "@nuxt/eslint-config": "^1.12.1",
    "@nuxt/kit": "^3.19.2",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^3.19.2",
    "@nuxt/test-utils": "^3.15.4",
    "@types/node": "^22.10.2",
    "changelogen": "^0.5.7",
    "concurrently": "^9.1.2",
    "eslint": "^9.17.0",
    "nuxt": "^3.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.2.0"
  }
}