{
  "name": "nuxt-oidc-auth",
  "private": false,
  "version": "0.12.5",
  "description": "OIDC authentication module for Nuxt SSR",
  "repository": "itpropro/nuxt-oidc-auth",
  "homepage": "https://github.com/itpropro/nuxt-oidc-auth#readme",
  "license": "MIT",
  "packageManager": "pnpm@9.0.6",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./package.json": "./package.json",
    "./runtime/*": "./dist/runtime/*"
  },
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build && pnpm client:build",
    "client:build": "nuxi generate client",
    "client:dev": "nuxi dev client --port 3300",
    "dev": "nuxi dev playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground && nuxi prepare client",
    "release": "pnpm lint && pnpm prepack",
    "lint": "eslint ."
  },
  "dependencies": {
    "@nuxt/devtools-kit": "^1.7.0",
    "consola": "^3.2.3",
    "defu": "^6.1.4",
    "h3": "^1.11.1",
    "jose": "^5.2.3",
    "ofetch": "^1.3.4",
    "scule": "^1.3.0",
    "sirv": "^2.0.4",
    "ufo": "^1.5.3",
    "uncrypto": "^0.1.3",
    "unstorage": "^1.10.2"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.7.0",
    "@nuxt/devtools-ui-kit": "^1.7.0",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/kit": "^3.19.2",
    "@nuxt/module-builder": "^0.6.0",
    "@nuxt/schema": "^3.19.2",
    "@types/node": "^20.19.17",
    "changelogen": "^0.5.5",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "nuxt": "^3.19.2",
    "typescript": "^5.9.2",
    "vue-tsc": "^2.2.12"
  }
}